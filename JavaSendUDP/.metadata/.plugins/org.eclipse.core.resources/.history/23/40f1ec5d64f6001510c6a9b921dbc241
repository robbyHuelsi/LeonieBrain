package udp;

public class Tank extends Unit {
	public int armor;

	public Tank(int hp, int damage, int attack, int speed, int armor) {
		super(hp, damage, attack, speed);
		this.armor = armor;
	}

	@Override
	public boolean shoot(Unit target) {
		// TODO Auto-generated method stub
		return super.shoot(target);
	}
	
	

}
